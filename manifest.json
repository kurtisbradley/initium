{
    "name": "Startpage",
    "description": "Override the new tab page with custom start page.",
    "version": "1.0",
    "incognito": "split",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}